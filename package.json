{
	"name": "@fanstrike/bank-launch",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"format": "bun run format:prettier && bun run format:biome",
		"format:prettier": "bunx --bun prettier --write '**/*.{md,mdx,yaml,yml}'",
		"format:biome": "biome format --write .",
		"lint": "biome check .",
		"lint:fix": "biome check . --fix",
		"lint:fix:unsafe": "biome check . --fix --unsafe",
		"type-check": "tsgo --noEmit",
		"test": "bun test"
	},
	"dependencies": {
		"@commander-js/extra-typings": "14.0.0",
		"@metaplex-foundation/genesis": "0.16.1",
		"@metaplex-foundation/mpl-token-metadata": "3.4.0",
		"@metaplex-foundation/mpl-toolbox": "0.10.0",
		"@metaplex-foundation/umi": "1.4.1",
		"@metaplex-foundation/umi-bundle-defaults": "1.4.1",
		"@types/node": "24.10.9",
		"date-fns": "4.1.0",
		"tslog": "4.10.2",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@tsconfig/strictest": "2.0.8",
		"@types/bun": "1.3.7",
		"@typescript/native-preview": "latest",
		"knip": "5.82.1",
		"lefthook": "2.0.16",
		"prettier": "3.8.1",
		"typescript": "5.9.3"
	},
	"packageManager": "bun@1.3.7"
}
